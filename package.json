{
  "name": "nodejs-webapi",
  "version": "0.0.1",
  "scripts": {
    "watch": "nodemon --exec ts-node src/main.ts",
    "test": "jest"
  },
  "dependencies": {
    "@types/express": "^4.11.0",
    "@types/jest": "^22.0.1",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "inversify": "^4.9.0",
    "inversify-express-utils": "^5.2.0",
    "mssql": "^4.1.0",
    "reflect-metadata": "^0.1.12",
    "ts-jest": "^22.0.1",
    "typeorm": "^0.1.11",
    "typescript": "^2.6.2"
  },
  "devDependencies": {
    "nodemon": "^1.14.11",
    "ts-node": "^4.1.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
